@prefix ottr: <http://ns.ottr.xyz/0.4/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ax: <http://tpl.ottr.xyz/owl/axiom/0.1/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rstr: <http://tpl.ottr.xyz/owl/restriction/0.1/> .
@prefix o-rdfs: <http://tpl.ottr.xyz/rdfs/0.2/> .

@prefix o-sdir: <https://www.sdir.no/SFI-model/ottr#> .
@prefix sdir: <https://www.sdir.no/SFI-model#> .


o-sdir:CreateRelation[owl:Class ?group, owl:Class ?parent, ? ?label, ? xsd:string ?code] :: {
    # Define group and connect to parent
	# define code and label for class
	
    ax:SubClassOf(?group, ?parent), 
	ottr:Triple(?group, sdir:code, ?code),
	ottr:Triple(?group, rdfs:label, ?label)
	
}.


o-sdir:GroupBelonging[owl:Class ?group, owl:Class ?overview_group] :: {
	# Defines in one of the four main groups
	
	ax:SubClassOf(?group, ?overview_group)
}.
